{
  "name": "generator-tspkg",
  "version": "1.2.7",
  "description": "Yeoman generator for TypeScript packages",
  "files": [
    "generators",
    "package.json",
    "yarn.lock"
  ],
  "keywords": [
    "yeoman-generator",
    "typescript",
    "ts",
    "package"
  ],
  "main": "generators/app/index.js",
  "engines": {
    "node": ">=10.15.3 <11"
  },
  "config": {
    "engine-strict": true
  },
  "scripts": {
    "yo": "node node_modules/yo/lib/cli.js ./generators/app",
    "yo:debug": "node --inspect node_modules/yo/lib/cli.js ./generators/app",
    "format": "prettier --write \"**/*.{js,json}\""
  },
  "dependencies": {
    "@selinarnd/generator-utils": "^1.2.1",
    "validator": "^10.11.0",
    "yeoman-generator": "^3.2.0"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^3.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "yo": "^2.0.5"
  }
}
